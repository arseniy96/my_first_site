<h1>New Question</h1>

<%= form_for :question, url: questions_path do |f| %>
    <p>
      <%= f.label :question %><br/>
      <%= f.text_area :question %>
    </p>
    <p>
      <%= f.label :username %><br/>
      <%= f.text_field :username %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>